{
  "name": "fraxioned-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run copy-assets",
    "copy-assets": "copyfiles -u 3 src/main/email/assets/**/* dist/public",
    "copy-assets:watch": "copyfiles -u 3 src/main/email/assets/**/* dist/public --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start-assets:dev": "npm-run-all --parallel nest:watch copy-assets:watch",
    "start:dev": "concurrently \"nest start --watch\" \"npm run copy-assets:watch\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "nest:watch": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "aws-sdk": "^2.1681.0",
    "axios": "^1.7.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "fraxioned-api": "file:",
    "glob": "^10.4.1",
    "handlebars": "^4.7.8",
    "moment-timezone": "^0.5.45",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.14",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.10",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^services/(.*)$": "<rootDir>/main/service/$1",
      "^entities/(.*)$": "<rootDir>/main/entities/$1",
      "^config/(.*)$": "<rootDir>/main/config/$1",
      "^controllers/(.*)$": "<rootDir>/main/controller/$1",
      "^dto/(.*)$": "<rootDir>/main/dto/$1",
      "^modules/(.*)$": "<rootDir>/main/modules/$1",
      "^database/(.*)$": "<rootDir>/main/database/$1",
      "^commons/(.*)$": "<rootDir>/main/commons/$1",
      "^swagger/(.*)$": "<rootDir>/main/swagger/$1",
      "^tests/(.*)$": "<rootDir>/tests/$1"
    }
  }
}
